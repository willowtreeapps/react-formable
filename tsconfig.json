{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./dist/",
    "lib": ["es2015", "dom"],
    "sourceMap": true,
    "allowJs": false,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "declarationDir": "./dist/"
  },
  "include": ["src/**/*"]
}
